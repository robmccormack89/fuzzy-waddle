<!DOCTYPE html>
<html {{site.language_attributes}}>
  <head>
  <meta charset="{{site.charset}}" />
  <meta name="viewport" content="width=device-width, initial-scale=1">
  {{ function('wp_head') }}
  </head>
  
  <style>
    
  
    .uk-offcanvas-bar {
        background: #24272f;
    }
    .nav-bottom-area .uk-navbar-nav {
        margin-left: -20px;
    }
    .nav-bottom-area .uk-navbar-item, .nav-bottom-area .uk-navbar-nav>li>a, .nav-bottom-area .uk-navbar-toggle {
        min-height: 65px;
        padding: 0 20px;
        font-size: 1rem;
    }
    .uk-logo.uk-navbar-item {
        min-height: 60px;
    }
    .top-bar-center {
        padding: 5px 0;
    }
    .slide-content-container {
        height: 100%;
    }
    .uk-logo {
      width: 350px;
    }
    .uk-background-primary {
      background-color: #01a4fb;
    }
    .uk-subnav-pill>.uk-active>a {
      background-color: #01a4fb;
    }
    .uk-button-primary {
      background-color: #01a4fb;
    }
    .uk-text-primary {
      color: #01a4fb!important;
    }
    .uk-link, a {
      color: #01a4fb;
    }
    .uk-button-primary:focus, .uk-button-primary:hover {
        background-color: #e41a1a;
    }
    .uk-button-secondary {
        background-color: #e41a1a;
        color: #fff;
    }
    .uk-button {
        border-radius: 30px;
        font-weight: 700;
        text-transform: capitalize;
    }
    .uk-subnav>*>:first-child {
        text-transform: capitalize;
        font-weight: 700;
    }
    .uk-button-large {
        padding: 0 30px;
        line-height: 45px;
        font-size: .875rem;
    }
    .uk-light .uk-button-primary {
        background-color: #e41a1a;
        color: #fff;
    }
    .uk-section-secondary {
        background: #24272f;
    }
    .uk-section.uk-section-secondary.darker {
        background-color: #1e2027;
    }
    .pull-left-15 {
        margin-left: -15px;
    }
    .hero-overlay {
      background-color: #01a4fb78;
    }
    .testimonials-overlay {
      background-color: #01a4fb78;
    }
    
    /* product quick call */
    #QuickCallEnquiryContainer {
        text-align: center;
        width: 100%;
        height: 60px;
        line-height: 60px;
        margin: 0 auto;
        position: fixed;
        left: 0;
        z-index: 999;
        bottom: 0;
        background: #e41a1a;
        color: #fff;
    }
     #QuickCallEnquiryContainer a {
    	 color: #fff;
    }
    #QuickCallEnquiryContainer a.close {
        position: relative;
        float: left;
        top: -15px;
        left: -7px;
        z-index: 99;
        font-weight: bold;
        font-size: 16px;
        -webkit-border-radius: 20px;
        -moz-border-radius: 20px;
        border-radius: 20px;
        padding: 2px 5px 2px 6px;
        line-height: 1em;
        text-align: center;
        background: #01a4fb;
        border: 4px solid #fff;
        cursor: pointer;
    }
     #CallNowButton {
    	 width: 100%;
    	 height: 100%;
    	 display: block;
    }
     @media screen and (min-width: 651px) {
    	 #QuickCallEnquiryContainer {
    		 display: none !important;
    	}
    }
     
    
    
    /* UIKit based resonsive breakpoints */
    @media only screen and (max-width:479px) {
      .uk-logo {
          width: 270px;
      }
      .testimonials-slider-heading {
        
      }
    }
    
    @media only screen and (min-device-width:480px) and (max-device-width:767px) {
      .uk-logo {
          width: 270px;
      }
    }
    
    @media only screen and (min-device-width:768px) and (max-device-width:959px) {
    
    }
    
    @media only screen and (min-device-width:960px) and (max-device-width:1199px) {
      
    }
    
    @media only screen and (min-width:1200px) {
      
    }
    
    /* custom breakpoints */
    @media only screen and (min-width:980px) {
      
    }
    
    @media only screen and (min-device-width:960px) {
      
    }
    
  </style>
  
  <body class="{{ body_class }}" data-template="base.twig">
    
    <div id="SiteWrapper" class="site-wrapper">
      
      {% block header %}
      {% include 'header.twig' %}
      {% endblock %}

      <main role="main" id="MainContent">
        {% block main %}
        <p>Sorry, Nothing to Display!!</p>
        {% endblock %}
      </main>

      {% block footer %}
      {% include 'footer.twig' %}
      {% endblock %}

    </div>
    
    {% include 'quickcall.twig' %}
    
    {{ function('wp_footer') }}
    
  </body>
</html>